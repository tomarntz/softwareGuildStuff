
@{
    ViewBag.Title = "MultipleObjectsBinding";
}

<h2>MultipleObjectsBinding</h2>

@using (Html.BeginForm("MultipleObjectsBinding", "Home", FormMethod.Post))
{
    <div>
        <div class="form-group">
            <label>City</label>
            <input type="text" name="[0].city" class="form-control"/>
        </div>
        <div class="form-group">
            <label>State</label>
            <input type="text" name="[0].State" class="form-control"/>
        </div>
        <div class="form-group">
            <label>ZipCode</label>
            <input type="text" name="[0].ZipCode" class="form-control"/>
        </div>
        <div class="form-group">
            <label>City</label>
            <input type="text" name="[1].city" class="form-control"/>
        </div>
        <div class="form-group">
            <label>State</label>
            <input type="text" name="[1].State" class="form-control"/>
        </div>
        <div class="form-group">
            <label>ZipCode</label>
            <input type="text" name="[1].ZipCode" class="form-control"/>
        </div>
        <button>Submit</button>
    </div>
}